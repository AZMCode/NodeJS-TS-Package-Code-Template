{
  "inputFiles": ["./src","./*.md"],
  "exclude": ["./**/*.test.*"],
  "media": "docs/Static",
  "theme": "markdown",
  "mdEngine": "github",
  "mdHideSources": true,
  "namedAnchors": true,
  "includeVersion": true,
  "readme": "none",
  "plugin": "typedoc-plugin-markdown",
  "mode": "modules",
  "out": "docs/Markdown",
  "disableOutputCheck": true
}
